{
    "compilerOptions": {
        "moduleResolution": "node",
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "src/*"
            ],
            "vue": [
                "node_modules/vue/dist/vue.esm-browser.js"
            ]
        },
        "types": [
            "vue"
        ]
    },
    "include": [
        "src/**/*.ts",
        "src/**/*.vue"
    ],
    "exclude": [
        "node_modules"
    ]
}